<core:FragmentDefinition height="100%" xmlns:rte="sap.ui.richtexteditor" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog id="idMassProcessNoteDialog" title="{i18n>master.frag.title.massProcess}" contentHeight="40%" contentWidth="40%">
		<content>
			<rte:RichTextEditor id="idRichTextNoteEditor"
										value="{masterView>/Notingstring}"
										width= "100%" 
										height="15rem" 
										customToolbar="true"
										showGroupFont= "true"
										showGroupLink= "true"
										showGroupInsert="true">
									</rte:RichTextEditor>	
		</content>
		<buttons>
			<Button text="{i18n>master.frag.action.process}"  press="onMultiProcess" type="Accept"></Button>
			<Button text="{i18n>master.frag.action.cancel}" press="onCancelMultiProcess"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>
